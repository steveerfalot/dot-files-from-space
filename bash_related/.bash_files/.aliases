#Servers Your Way Variables
export SYW_HOME="/workspace/serversYourWay"

#Debt Tracker Variables
export DT_HOME="/workspace/debttracker"

#chrome
alias chrome="nohup google-chrome ----profile-directory=\"Testing\" --user-data-dir=\"/home/steveerf/.chrome-testing\" --disable-web-security -incognito http://localhost:8500/bullhornstaffing/bhlogin.cfm"

#ide
alias webstorm="sh /ide/WebStorm-139.252/bin/webstorm.sh"
